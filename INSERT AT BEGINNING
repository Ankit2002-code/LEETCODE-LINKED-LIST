	#include<bits/stdc++.h>
	using namespace std;
	struct Node{
	    public:
	    int data;
	    Node*next;
	};
	Node* insertAtBeginning(int val, Node*head) {
	    Node* new_node = new Node;
	    new_node->data = val;
	    new_node->next = head;
	    head = new_node;
	    return head;
	}
	int main(){
	    Node*head=nullptr;
	     head = insertAtBeginning(10,head);
	     cout<<head->data;
	     return 0;
}
